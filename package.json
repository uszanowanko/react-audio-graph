		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^2.1.0",
		"file-loader": "^3.0.1",
		"gh-pages": "^2.0.1",
		"html-webpack-plugin": "^3.2.0",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"style-loader": "^0.23.1",
		"webpack": "^4.28.3",
