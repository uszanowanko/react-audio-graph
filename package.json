{
	"name": "react-audio-graph",
	"version": "0.0.1",
	"description": "React component for visualizing audio buffers as waveform graph",
	"main": "dist/index.js",
	"scripts": {
		"start": "webpack-dev-server --mode development",
		"transpile": "babel src -d dist --copy-files",
